{
    "name": "yajra/acl",
    "description": "Simple Role Permission Acl for Laravel Framework.",
    "keywords": [
        "yajra",
        "acl"
    ],
    "homepage": "https://github.com/yajra/acl",
    "license": "MIT",
    "authors": [
        {
            "name": "Arjay Angeles",
            "email": "aqangeles@gmail.com",
            "homepage": "http://yajrabox.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.9",
        "illuminate/support": "~5.1",
        "illuminate/database": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Yajra\\Acl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yajra\\acl\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
